   <div id="systemgoals" class="tab-pane fade">
      {{{ansi-format text=this.char.custom.goals}}}
   </div>
  
  <div id="connections" class="tab-pane fade">
     {{{ansi-format text=this.char.custom.connections}}}
  </div>

  <div id="systemdowayne" class="tab-pane fade">
     {{#if this.char.custom.house_list}}
         <h3>House Management</h3>
         <ProfileDowayne @house={{this.char.custom.house_list}} />
     {{/if}}
  </div>

  <div id="systemmarque" class="tab-pane fade">
      <div class="xp-ability-name"><br>Current progress in percent:</div>
      <br>
      <div class="xp-progress">
         <ProgressBar @currentValue={{{this.char.custom.marque}}} @maxValue="100" @bgColor="bg-success" @showLabel=true>
         </ProgressBar>
         <div class="progress-value">{{{this.char.custom.marque}}}%</div>
      </div>

  </div>

  <div id="pentraits" class="tab-pane fade">
    <center>
       <div class="col col-md-9 col-sm-12">
          <div class="row pen-traits-box">
            <PenTraitsTable @title="Traits" @list={{this.char.custom.pen_traits}} @max={{20}} />
          </div>
       </div>
    </center>
 </div>

  <div id="systemrenown" class="tab-pane fade">
     <div class="char-ability-title">Current Season</div>
     <RenownGained @list={{this.char.custom.renown_gained}} @total={{this.char.custom.gained_total}}/>
 </div>

  <div id="economy" class="tab-pane fade">
        {{#if this.char.custom.limit}}
           <div class="row">
               <div class="col col-md-6 col-sm-12">
                   <EconChart @list={{this.char.custom.econ_chart}} />
               </div>
                <div class="col col-md-6 col-sm-12">
                   <EconStats @limit={{this.char.custom.limit}} @info={{this.char.custom.block_info}} @expiry={{this.char.custom.block_expiry}} @modifiers={{this.char.custom.modifiers}} @name={{this.char.name}} />
                </div>
               {{#if this.char.custom.econ_chart}}
               {{else}}
                    <div align=left> <button name="econset" {{action 'econSet' char.name}} class="btn btn-secondary">Set your limit</button></div>
               {{/if}}
           </div>
        {{/if}}
  </div>

